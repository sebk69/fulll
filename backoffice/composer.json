{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "8.4.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/console": "7.1.*",
        "symfony/flex": "2.4.*",
        "symfony/runtime": "7.2.*"
    },
    "require-dev": {
        "behat/behat": "*",
        "phpunit/dbunit": "*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "allow-plugins": {
            "ocramius/package-versions": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "scripts": {
        "behat": "vendor/bin/behat",
        "auto-scripts": {
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "autoload": {
        "psr-4": {
            "Fulll\\": "src/",
            "App\\": "src/Infra/App/"
        }
    }
}
